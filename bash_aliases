alias c="clear"

alias l="ls"
alias la="ls -a"
alias ll="ls -la"

alias ..="cd ../"
alias ..2="cd ../../"
alias ..3="cd ../../../"
alias ..4="cd ../../../../"
alias ..5="cd ../../../../../"

alias sba='source ~/.bash_aliases'
alias sp='source ~/.profile'

alias v='nvim'

alias pn='bash ~/.scripts/sync/push_notes.sh'
